{
  "name": "vendor-server-template",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && c8 node --test -r ts-node/register \"test/**/*.ts\"",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build": "tsc && npm run copy-assets",
    "build:ts": "tsc && npm run copy-assets",
    "watch:ts": "tsc -w && npm run copy-assets",
    "copy-assets": "cpx \"src/migrations/**/*.sql\" dist/migrations",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "nodemon -L --watch dist --exec \"fastify start -l info -P dist/app.js\"",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org officex --project vendor-officex ./dist && sentry-cli sourcemaps upload --org officex --project vendor-officex ./dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-iam": "^3.857.0",
    "@aws-sdk/client-s3": "^3.857.0",
    "@aws-sdk/credential-provider-node": "^3.857.0",
    "@aws-sdk/s3-presigned-post": "^3.846.0",
    "@aws-sdk/s3-request-presigner": "^3.846.0",
    "@dfinity/principal": "^2.4.1",
    "@fastify/autoload": "^6.0.0",
    "@fastify/cors": "^11.0.1",
    "@fastify/http-proxy": "^11.3.0",
    "@fastify/sensible": "^6.0.0",
    "@fastify/static": "^8.2.0",
    "@noble/ed25519": "^2.3.0",
    "@noble/hashes": "^1.8.0",
    "@officexapp/types": "^0.0.195",
    "@sentry/cli": "^2.51.1",
    "@sentry/node": "^10.3.0",
    "@sentry/profiling-node": "^10.3.0",
    "base64-js": "^1.5.1",
    "better-sqlite3": "^12.2.0",
    "bip39": "^3.1.0",
    "cross-env": "^10.0.0",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.0",
    "ethers": "^6.15.0",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.4.0",
    "fastify-cron": "^1.4.0",
    "fastify-plugin": "^5.0.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "uninstall": "^0.0.0",
    "viem": "^2.33.1",
    "yauzl": "^3.2.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.1.0",
    "@types/node-cache": "^4.1.3",
    "@types/pg": "^8.15.4",
    "@types/yauzl": "^2.10.3",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "cpx": "^1.5.0",
    "fastify-tsconfig": "^3.0.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.4.0",
    "typescript": "~5.8.2"
  },
  "prettier": {
    "printWidth": 120
  }
}
