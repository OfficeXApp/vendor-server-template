# PostgreSQL/TimescaleDB Credentials
# These are used by the 'db' service in docker-compose.yml to initialize the database.
# IMPORTANT: CHANGE 'your_secure_db_password' to a strong, unique password!
POSTGRES_USER=postgres # Default user for TimescaleDB/PostgreSQL Docker image
POSTGRES_PASSWORD=your_secure_db_password
POSTGRES_DB=myapp_db # The name of the database your application will use

# Database Connection URL for your Node.js Fastify application
# This URL is constructed using the variables above and the Docker service name 'db'.
# Your Node.js app will read this DATABASE_URL environment variable.
DATABASE_URL=postgres://${POSTGRES_USER}:${POSTGRES_PASSWORD}@db:5432/${POSTGRES_DB}


# Traefik Configuration for SSL
TRAEFIK_ENABLED=false
LE_EMAIL=youremail@email.com
